fn TryStakeVampire(vampire : IEnemy)
{
	if(vampire.GetHP() < 25)
	{
		say "You drive a wooden stake into the vampire's heart!";
		vampire.Kill();
	}
	else
	{
		say "The vampire is too strong. You have to weaken it more.";
	}
}

