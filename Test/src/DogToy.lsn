using "test3";
using "CatToy";

struct DogToy{
	Toy : Item,
	Owner : Dog
}

handle hLvlDog : hDog hLvlChar;

handle hDog : hAnimal;

fn ToCatToy(d : DogToy) -> CatToy
{
	return new CatToy(d.Toy, ToCat(d.Owner));
}


fn FeedDog(dog: hDog)
{
	say "Feeding dog.";
	PetAnimal(dog);
}


fn HandlePolymorphismTest(dog: hLvlDog)
{
	FeedDog(dog);
	PetAnimal(dog);
	MoveLvlObject(dog, 0, 1.5);
}